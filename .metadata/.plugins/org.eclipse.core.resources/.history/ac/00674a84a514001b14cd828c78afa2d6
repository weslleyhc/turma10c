package br.com.ecommerce.implementacao;

import br.com.ecommerce.beans.Venda;
import br.com.ecommerce.util.Magica;

public class ImplementarVendas {

	public static void main(String[] args) {
		Venda vendas = new Venda(
				Magica.i("NotaFiscal"),
				Magica.f("Total"),
				Magica.f("Desconto"),
				Magica.s("Data"),
				new Cliente(
						Magica.s("Cliente"),
						new Produto (
								Magica.s("Produto")
								)						
						)
				);

	}

}
