package br.com.casageral.modelo;

public class Loja {
	private String marca;
	private float valor;
	private boolean status;
	private int canal;
	private int volume;
	
	public void preencherMarca(String pMarca) {
		marca = pMarca.toUpperCase();
	}
	public String retornaMarca() {
		return marca;
	}
	
	
	

}
